@model AspnetOkta.Domain.MyUser

<h2>User Profile</h2>
@if(@Model == null){
  <p>Sorry no user found</p>
}else{
  <ul>
    <li>
      <strong>First Name:</strong>
      @Model.FirstName
    </li>
    <li>
      <strong>Last Name:</strong>
      @Model.LastName
    </li>
    <li>
      <strong>Favorite Color:</strong>
      @Model.FavoriteColor
    </li>
  </ul>
}
